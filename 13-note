import turtle
t = turtle.Turtle()

t.home()
t.clear()

for i in range(4):
  t.forward(100)
  t.left(90)

t.home()
t.left(90)
t.forward(110)

for i in range(8):
  t.forward(100)
  t.forward(-100)
  t.left(-90)
  t.forward(10)
  t.left(90)
  
t.home()
t.clear()

for j in range(2):
 for i in range(4):
  t.forward(100)
  t.left(90)
 
  t.left(90)
  t.forward(120)
 
 for i in range(6):
   t.forward(100)
   t.forward(-100)
   t.left(-90)
   t.forward(20)
   t.left(90)
 
   t.forward(-120)
   t.left(-90)
   t.forward(-240)

print('hello')

def yline(x,y,a):
  t.up()
  t.home()
  t.forward(x)
  t.left(90)
  t.forward(y)
  t.down()
  t.forward(a)

t.clear()
rect(0,0,100)
rect(-120,0,100)

for x in [0,20,40,60,80,100]:
  yline(x,120,100)
  yline(x-120,120,100)
  
